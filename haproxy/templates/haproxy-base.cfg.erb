<% haproxy_defaults_options.sort.each do |key,val| -%>
<% if val.is_a?(Array) -%>
<% val.each do |item| -%>
  <%= key %>  <%= item %>
<% end -%>
<% else -%>
  <%= key %>  <%= val %>
<% end -%>
<% end -%>